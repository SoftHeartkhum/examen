import numpy as np
from scipy.sparse import csr_matrix

# Crear matriz A en formato disperso CSR
data_A = np.array([1, 2, 3])
rows_A = np.array([0, 1, 2])
cols_A = np.array([0, 1, 2])
A = csr_matrix((data_A, (rows_A, cols_A)), shape=(3, 3))

# Crear matriz B en formato disperso CSR
data_B = np.array([4, 5, 6])
rows_B = np.array([0, 1, 2])
cols_B = np.array([0, 1, 2])
B = csr_matrix((data_B, (rows_B, cols_B)), shape=(3, 3))

# Multiplicar las matrices dispersas
C = A.dot(B)

# Convertir el resultado a una matriz densa para la visualizaci√≥n
C_dense = C.toarray()
print(C_dense)
